{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noEmit":true,
    "allowImportingTsExtensions":true,
    "types": [
      "@nuxtjs/axios",
      "@nuxtjs/auth-next",
    ]
  }
}